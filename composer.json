{
  "name": "ergebnis/factory-bot",
  "type": "library",
  "description": "Provides an entity factory for doctrine/orm entities that can be used in tests.",
  "keywords": [
    "doctrine",
    "entity",
    "factory"
  ],
  "homepage": "https://github.com/ergebnis/factory-bot",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "doctrine/annotations": "^1.7.0",
    "doctrine/collections": "^1.0.0",
    "doctrine/orm": "^2.6.3",
    "ergebnis/classy": "~0.5.2",
    "fzaninotto/faker": "^1.9.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.3.2",
    "ergebnis/license": "~0.1.0",
    "ergebnis/php-cs-fixer-config": "^2.1.1",
    "ergebnis/phpstan-rules": "~0.14.4",
    "ergebnis/test-util": "~0.9.1",
    "infection/infection": "~0.15.3",
    "jangregor/phpstan-prophecy": "~0.6.2",
    "phpstan/extension-installer": "^1.0.4",
    "phpstan/phpstan": "~0.12.18",
    "phpstan/phpstan-deprecation-rules": "~0.12.2",
    "phpstan/phpstan-doctrine": "~0.12.10",
    "phpstan/phpstan-phpunit": "~0.12.7",
    "phpstan/phpstan-strict-rules": "~0.12.2",
    "phpunit/phpunit": "^8.5.3",
    "psalm/plugin-phpunit": "~0.10.0",
    "vimeo/psalm": "^3.11.2"
  },
  "config": {
    "platform": {
      "php": "7.2.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\FactoryBot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\FactoryBot\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/factory-bot/issues",
    "source": "https://github.com/ergebnis/factory-bot"
  }
}
